{
  "name": "palm-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "compile": "graphql-codegen",
    "watch": "graphql-codegen -w",
    "bundle": "bundle install",
    "clean": "watchman watch-del $PWD ; watchman watch-project $PWD",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "postinstall": "node_modules/.bin/rn-nodeify --install crypto,http,https,assert,url,stream,events --hack",
    "android:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android:testnet": "ENVFILE=.env.testnet react-native run-android --variant=palmTestnetDebug",
    "android:testnet:release": "ENVFILE=.env.testnet react-native run-android --variant=palmTestnetRelease",
    "android:mainnet": "ENVFILE=.env.mainnet react-native run-android --variant=palmMainnetDebug",
    "android:mainnet:release": "ENVFILE=.env.mainnet react-native run-android --variant=palmMainnetRelease",
    "android:testnet:build": "cd android && ./gradlew assemblePalmTestnetRelease",
    "android:mainnet:build": "cd android && ./gradlew assemblePalmMainnetRelease",
    "android:mainnet:beta": "npm run clean; fastlane android beta --env mainnet",
    "android:testnet:beta": "npm run clean; fastlane android beta --env testnet",
    "android:testnet:codePush": "npm run clean; fastlane android codepush --env testnet",
    "android:mainnet:codePush": "npm run clean; fastlane android codepush --env mainnet",
    "ios:testnet": "react-native run-ios --scheme 'PalmApp-Testnet' --simulator='iPhone 14'",
    "ios:mainnet": "react-native run-ios --scheme 'PalmApp' --simulator='iPhone 14'",
    "ios:pod": "npm run bundle; npm run ios:pod-install",
    "ios:pod-install": "cd ios; pod install --repo-update;",
    "ios:mainnet:beta": "npm run clean; fastlane ios beta --env mainnet",
    "ios:testnet:beta": "npm run clean; fastlane ios beta --env testnet",
    "ios:testnet:codePush": "npm run clean; fastlane ios codepush --env testnet",
    "ios:mainnet:codePush": "npm run clean; fastlane ios codepush --env mainnet"
  },
