{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "libs/*": ["./libs/*"],
      "hooks/*": ["./hooks/*"],
      "store/*": ["./store/*"],
      "assets/*": ["./assets/*"],
      "types": ["./types"],
      "consts": ["./consts"],
      "components": ["./components"],
      "graphql": ["./graphql/*"]
    },
    "types": [
      "react-native",
      "jest",
      "styled-components-react-native",
    ],
    "esModuleInterop": true,
    "noImplicitAny": false,
  }
}
